{
  "name": "n8n-nodes-kalshi",
  "version": "1.0.0",
  "description": "n8n community node for Kalshi prediction markets - trade, analyze, and automate prediction market strategies",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "kalshi",
    "prediction markets",
    "trading",
    "markets",
    "api",
    "automation",
    "forecasting",
    "betting"
  ],
  "license": "MIT",
  "homepage": "https://github.com/deepakdhaka-1/n8n-nodes-kalshi/#readme",
  "author": {
    "name": "Deepak Dhaka",
    "email": "gxyz717@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deepakdhaka-1/n8n-nodes-kalshi.git"
  },
  "bugs": {
    "url": "https://github.com/deepakdhaka-1/n8n-nodes-kalshi/issues"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "test": "echo \"No tests yet\" && exit 0"
